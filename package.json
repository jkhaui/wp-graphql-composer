{
  "name": "wp-graphql-composer",
  "version": "0.1.3",
  "description": "A light-weight framework for developing wordpress React-powered WordPress plugin and themes",
  "keywords": [
    "wp-graphql",
    "wordpress",
    "react",
    "apollo",
    "graphql",
    "react-apollo",
    "recompose"
  ],
  "author": {
    "name": "Geoff Taylor",
    "url": "https://axistaylor.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kidunot89/wp-graphql-composer.git"
  },
  "bugs": {
    "url": "https://github.com/kidunot89/wp-graphql-composer/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "templates",
    "scripts.js"
  ],
  "bin": {
    "wp-graphql-compose": "bin/compose.js"
  },
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "commander": "^2.19.0",
    "del-symlinks": "^0.1.2",
    "graphql": "^14.0.2",
    "inquirer": "^6.2.0",
    "make-symlinks": "^1.1.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.4",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-transition-group": "^2.5.0",
    "recompose": "^0.30.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "react-testing-library": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "proxy": "http://wpgraphql.example:8000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
