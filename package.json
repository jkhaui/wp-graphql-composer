{
  "name": "wp-graphql-composer",
  "version": "0.1.3",
  "description": "A library of customizable WordPress React-Apollo components",
  "keywords": [
    "wp-graphql",
    "wordpress",
    "react",
    "apollo",
    "graphql",
    "react-apollo",
    "recompose"
  ],
  "author": "Geoff Taylor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kidunot89/wp-graphql-composer.git"
  },
  "bugs": {
    "url": "https://github.com/kidunot89/wp-graphql-composer/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css watch-demo-css start-js",
    "watch-css": "npm run build-css && node-sass-chokidar src/lib/scss/ -o src/lib/ --watch --recursive",
    "build-css": "node-sass-chokidar src/lib/scss/ -o src/lib/",
    "watch-demo-css": "npm run build-css && node-sass-chokidar src/demo/scss/ -o src/demo/ --watch --recursive",
    "build-demo-css": "node-sass-chokidar src/demo/scss/ -o src/demo/",
    "build": "npm run build-css && rimraf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "build-demo": "npm-run-all -s build-css build-demo-css && react-scripts build && rm -rf demo && mv build demo",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "casual": "^1.5.19",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-testing-library": "^5.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "apollo-boost": "^0.1.16",
    "dotenv": "^6.1.0",
    "graphql": "^14.0.2",
    "html-to-react": "^1.3.3",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-live": "^1.11.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5",
    "react-transition-group": "^2.5.0",
    "recompose": "^0.30.0"
  },
  "proxy": {
    "/graphql": {
      "target": "http://wpgraphql.example:8000",
      "ws": true
    }
  },
  "homepage": "https://api.axistaylor.com/composer/"
}
